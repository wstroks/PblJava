package br.uefs.ecomp.samu.util;

/**
*
* @author Washington Pagotto Batista
*/

public class No {
    
	//atributos da classe
	private Object dados;
	private No next;
	
	//construtor da classe
	public No(Object dados)
	{
		this.dados=dados;
		next=null;
	}
   
	/**
     * Esse metodo é usado para fazer a modificação do atributo dados da classe
     * No, obrigatóriamente ele recebe um parametro que será o novo valor,
     * esse metodo não tem nenhum retorno.
     */
	public Object getDados()
	{
		return dados;
	}
	
	/**
     * Esse metodo é usado para verificar o campo next da classe Link, como ele
     * verifica um valor sempre deve ter um retorno, no caso, esse retorna um
     * objeto do tipo Link.
     */
	public No getNext()
	{
		return next;
	}
	
	/**
     * Esse metodo é usado para fazer a modificação do atributo dados da classe
     * Link, obrigatóriamente ele recebe um parametro que será o novo valor,
     * esse metodo não tem nenhum retorno.
     */
	public void setDados(Object dados)
	{
		this.dados=dados;
	}
	
	/**
     * Esse metodo é usado para fazer a modificação do atributo next da classe
     * No, obrigatóriamente ele recebe um parametro que será o novo valor,
     * esse metodo não tem nenhum retorno.
     */
	public void setNext(No next)
	{
		this.next=next;
	}
	
	/**
     * Esse metodo é usado para verificar o campo dados da classe No, como ele
     * verifica um valor sempre deve ter um retorno, no caso, esse retorna um
     * objeto do tipo object.
     */
	public Object getObject()
	{
		return dados;
	}
}


