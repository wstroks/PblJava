package br.uefs.ecomp.samu.util;
/**
*
* @author Washington Pagotto Batista
*/

public class MeuIterador implements Iterador{
	 
	 //atributos da classe
	 private No novo;
	
	 //construtor da classe
	 public MeuIterador(No novo)
	 {
		this.novo=novo;
		 
	 }
	
	@Override
	/**
     * Esse metodo é responsavel por verificar se há um proximo elemento na
     * lista, caso haja ele retorna um valor verdadeiro caso nao um valor falso.
     */
	public boolean temProximo() 
	 {
		// TODO Auto-generated method stub
		if(novo!=null)
		{
			return true;
		}
		
		return false;
	 }

	
	@Override
	 /**
     * Esse metodo é responsavel por fazer o retorno do conteudo de uma celula
     * da lista, nele é criado um novo objeto do tipo object, é feita a
     * verificação se há um proximo elemento com o metodo temProximo() caso
     * haja, esse objeto criado recebe o conteudo do link atual, e atual recebe
     * o seu proximo elemento, ao final o objeto criado é retornado com o
     * conteudo da celula, caso nao haja proximo é retornado um valor nulo.
     */
	public Object proximo() 
	{
		// TODO Auto-generated method stub
		Object auxiliar;
				
		if(!temProximo())
		{
			return null;
		}
		
		else
		{
			auxiliar=novo;
			novo=novo.getNext();
			return ((No) auxiliar).getDados();
		}
			
	
		
	 }
	
	
}
